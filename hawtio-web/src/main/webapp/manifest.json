{
  "manifest_version": 2,

  "name": "hawtio",
  "description": "a modular web console for managing your Java stuff",

  "version": "1.0.0",

  "update_url": "http://hawt.io/chrome/extension.xml",

  "icons": {
    "16": "img/logo-16px.png",
    "128": "img/logo-128px.png"
  },

  "permissions": [
    "http://*/",
    "https://*/"
  ],

  "app": {
    "launch": {
      "local_path": "index.html"
    }
  }
}
